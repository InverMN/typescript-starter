{
  "name": "application",
  "main": "dist/main.js",
  "type": "module",
  "scripts": {
    "script:append-import-extensions": "node ./scripts/append-extensions-to-import-paths.js",
    "build": "yarn run build:code",
    "build:types": "tsc --emitDeclarationOnly",
    "build:code": "tsc && yarn run script:append-import-extensions",
    "build:all": "yarn run build:types && yarn run build:code",
    "test": "yarn run test:units",
    "test:types": "tsc --noEmit",
    "test:units": "jest --notify",
    "test:mutation": "stryker run",
    "test:dev": "jest --watchAll",
    "test:all": "yarn run test:types && yarn run test:units && yarn run test:mutants",
    "lint": "eslint \"src/**\"",
    "prettier:base": "prettier --parser typescript",
    "prettier:check": "npm run prettier:base -- --list-different \"src/**/*.{ts,tsx}\"",
    "prettier:write": "npm run prettier:base -- --write \"src/**/*.{ts,tsx}\"",
    "start": "yarn run build:code && node dist/main.js",
    "start:dev": "nodemon --exec \"yarn run start\""
  },
  "devDependencies": {
    "@stryker-mutator/core": "^4.5.1",
    "@stryker-mutator/jest-runner": "^4.5.1",
    "@types/jest": "^26.0.22",
    "@types/node": "^14.14.37",
    "@typescript-eslint/eslint-plugin": "^4.20.0",
    "@typescript-eslint/parser": "^4.20.0",
    "eslint": "^7.23.0",
    "eslint-plugin-react": "^7.23.1",
    "filehound": "^1.17.4",
    "jest": "^26.6.3",
    "nodemon": "^2.0.4",
    "prettier": "^2.2.1",
    "ts-jest": "^26.5.4",
    "ts-node": "^9.1.1",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.2.3"
  },
  "dependencies": {}
}
